pipeline {
    agent any 
    stages {
        stage('Hello World') {
            steps {
                sh 'echo Hello World'
            }
        }
		stage('SCM-Checkout') {
            steps {
                sh 'echo SCM-Checkout'
            }
        }
		stage('Build-Test') {
            steps {
                sh 'echo Build-Test'
            }
        }
		stage('Archive') {
            steps {
                sh 'echo Archive'
            }
        }
    }
}
